# Streamlit Cloud Configuration
# Note: Streamlit Cloud has a 200MB file upload limit
# This config is optimized for cloud deployment

[server]
# Streamlit Cloud max is 200MB
maxUploadSize = 209715200  # 200MB
maxMessageSize = 209715200
port = 8501
address = "0.0.0.0"

[browser]
gatherUsageStats = false

[theme]
primaryColor = "#1f77b4"
backgroundColor = "#ffffff"
secondaryBackgroundColor = "#f0f2f6"
textColor = "#262730"
font = "sans serif"



